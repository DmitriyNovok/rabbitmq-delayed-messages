{
    "require": {
        "php": "8.1",
        "ext-amqp": "*",
        "php-amqplib/php-amqplib": "^2.12"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "disable-tls": true,
        "secure-http": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
